</script>
<link href="$SkinDirUrl/layout-print.css" rel="stylesheet" type="text/css" media="print" />
</head>
<body >
<div id='wrapper'>
<div id='header'>
<!--PageTopMenuFmt-->
	<div id='topnav'>
		<!--wiki:$Group.PageTopMenu $SiteGroup.PageTopMenu-->
		<div class='clearer'><!-- a clearer division --></div>
	</div>
<!--/PageTopMenuFmt-->
<!--PageHeaderFmt-->
	<div id='topheader' class='pageheader'> $LogoFmt
		<!--wiki:$Group.PageHeader $SiteGroup.PageHeader-->
	</div>
<!--/PageHeaderFmt-->
</div>
	<div class='clearer'><!-- a clearer division --></div>

	  <table id='midtbl' width='100%' cellspacing='0' cellpadding='0'>
		<tr>
<!--PageLeftFmt-->
		<td id='sidebarbox' valign='top'>
			<div id='sidebar'>
				<div id='sideheader' class='pageheader'> $LogoFmt
					<!--wiki:$Group.PageHeader $SiteGroup.PageHeader-->
				</div>
				<div id='sidebarsearch'>$SearchTagFmt</div>
				<!--wiki:$Group.SideBar $SiteGroup.SideBar-->
				<div id="sidebarfooter">
					<p><a href='$ScriptUrl/PmWiki.GeminiTwo'>GeminiTwo</a> 10b<br />
				  	Powered by <a href='$ScriptUrl/PmWiki.PmWiki'>PmWiki</a></p>
			</div>
		</div> </td>
<!--/PageLeftFmt-->

    <td id='main' valign='top' align='left'>
<!--PageTitleFmt-->
		<div id='titlebarbox'>
			<div id='titlebar'>
				<div id='pagegroup'><a href='$ScriptUrl/$PageGroup'>$PageGroup</a></div>
				<div id='pagetitle'>
				<!--wiki:$FullName-TitleBar--><h1><a class='titlelink' href='$PageUrl'>$Titlespaced</a></h1>
				</div>
			</div>
		 </div>
<!--/PageTitleFmt-->
		<div class='clearer'><!--end of page--></div>
		<div id='content'>
<!--PageRightFmt-->
			<div id='rightbar'><!--wiki:$FullName-RightBar $Group.RightBar $SiteGroup.RightBar--></div>
<!--/PageRightFmt-->
<!--PageText-->
			<div class='clearer'><!-- a clearer division --></div>
		</div>
<!--PageFootMenuFmt-->
		<div id='footnavbox'>
			<div id='footnav'><!--wiki:$Group.PageFootMenu $SiteGroup.PageFootMenu--></div>
		</div>
<!--/PageFootMenuFmt-->
		</td></tr>
	</table>
<!--PageFooterFmt-->
	<div id='footer'><!--wiki:$Group.PageFooter $SiteGroup.PageFooter--></div>
<!--/PageFooterFmt-->
</div>
</body>
</html>
