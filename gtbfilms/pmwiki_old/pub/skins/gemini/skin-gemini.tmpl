<html>
<head>
  <title>$WikiTitle - $Group - $Title</title>
 <style type="text/css" media="all">
 		@import "$SkinDirUrl/layout-main.css";
		@import "$SkinDirUrl/$LayoutCss";
		@import "$SkinDirUrl/$RightbarCss";
 </style>
  <link rel='stylesheet' href='$SkinDirUrl/$FontCss' type='text/css' media='screen' />
  <link rel='stylesheet' href='$SkinDirUrl/$ColorCss' type='text/css' media='screen' />
  <!--HeaderText-->
<script language="JavaScript" type="text/javascript">
